log:
    - name: root
      level: info
      formatter: "%d%T%m%n"
      appender: 
            - type: FileLogAppender
              file: log.txt
            - type: StdoutLogAppender
    - name: system
      level: error
      formatter: "%d%T%m"
      appender:
            - type: StdoutLogAppender
              

